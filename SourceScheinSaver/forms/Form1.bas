Private Sub Command0_Click()
'DoCmd.RunCommand acCmdRecordsGoToNew

Dim Rst9 As Recordset, DBS9 As Database, SQ9 As String, User As String, SQ1 As String, Datestuff As String, Rst1 As Recordset, PONum As String

Dim Items() As String, How As Long, i As Integer, a_counter As Integer



On Error GoTo error

User = GetUser()
Set DBS9 = CurrentDb()
SQ9 = "select id, recid, itemcode from Tbl_Items_promo"
SQ1 = "select id,[RecID Number], [Eligible SKU] from POP_notes"
Set Rst9 = DBS9.OpenRecordset(SQ9) ', dbOpenDynaset, dbSeeChanges)
Set Rst1 = DBS9.OpenRecordset(SQ1) ', dbOpenDynaset, dbSeeChanges)
'MsgBox Rst9.RecordCount

Rst9.MoveFirst
Rst1.MoveFirst

Do
If Rst1.AbsolutePosition < 0 Then
Exit Do
ElseIf Rst1.Fields(2) <> "" Then
Items = Split(Rst1.Fields(2), ";")

How = UBound(Items)

For a_counter = 0 To How - 1
     Rst9.AddNew
     Rst9.Fields(1) = Rst1.Fields(1)
     Rst9.Fields(2) = Items(a_counter)
     Rst9.Update
    
Next a_counter

Rst1.MoveNext
Else
Rst1.MoveNext
End If


Loop


error:
If Err.Number = 0 Then
Err.Clear
ElseIf Err.Number = 3021 Then
Resume Next
ElseIf Err.Number = 94 Then
Resume Next
ElseIf Err.Number = 9 Then
Resume Next
ElseIf Err.Number = 424 Then
Resume Next
ElseIf Err.Number = 3421 Then
Resume Next
Else
MsgBox Err.Description & " " & Err.Number
End If



