Private Sub Dt_Click()
config_log "Form_DefProF - Dt_Click"
On Error GoTo Err_Dt_Click

Dim dtEff As Date
Dim dtExp As Date
Dim stEff As String
Dim stExp As String
Dim stEffExp As String

dtEff = Me.Parent.EffDate
stEff = Format(dtEff, "mmmm dd")
dtExp = Me.Parent.ExpDate
stExp = Format(dtExp, "mmmm dd, yyyy")
stEffExp = stEff & " - " & stExp

If IsNull(Me!DtRange) Then Me!DtRange = stEffExp

DftRange.SetFocus

Exit_Dt_Click:
    Exit Sub

Err_Dt_Click:
    MsgBox Err.Description
    Resume Exit_Dt_Click
    
End Sub

