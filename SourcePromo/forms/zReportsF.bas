'Del/Make [zItemCompile], Append [RecID].[zPromo] to [RecID].[zItemCompile]
DoCmd.RunMacro "zItemCompileM" 'Creates a list of promotions for the period
'with two fields: [CodeID] an autonumber field and [RecID] links to a promotion.
'Now Count the records in this newly created [zItemCompile].
intProCnt = DCount("[RecID]", "zItemCompile", _
"[EffQtr] =" & ZEffQtr() & " AND [EffYr] =" & ZEffYr())
    Z_RecCnt = intProCnt 'Variable for Total Promotions
    'MsgBox "Total Promotions = " & ZRecCnt()
    'MsgBox intProCnt & " records for Q" & ZEffQtr() & " " & ZEffYr()

'MsgBox "Flag 2"
'Let's start going thru the promotions one at a time.
For x = 1 To intProCnt
'****************************************************
        Z_CurRec = x
        [Forms]![zReportsF]![CurRec] = ZCurRec() 'Show progress on form
        [Forms]![zReportsF]![RecCnt] = ZRecCnt()
        Me.Refresh 'Requery the "Meter"
        Me.Repaint
        'MsgBox "Current Record = " & ZCurRec()
    '[Forms]![zReportsF]![Meter] = ZCurRec() & ", " & ZRecCnt()
    'Now, Get the xth [RecID], that is, the link to the promotion
    intProRecID = DLookup("[RecID]", "zItemCompile", "[CodeID] =" & x)
        'MsgBox x & " [RecID] is " & intProRecID
    Z_RecID = intProRecID
    'Now let's start collecting the HSI Item Codes for the current promotion
    'Count how many records in [zItemSub] have the intProRecID [RecID]?
    If IsNull(DCount("[CodeID]", "zItemSub", "[RecID] =" & intProRecID)) Then
       'Next x, that is, if there aren't any valid item codes for this promotion
       'then skip to the next promotion.
    Else
        'There are any valid item codes for this promotion...
        'Count the number of item codes to be looked at.
        intItmCnt = DCount("[CodeID]", "zItemSub", "[RecID] =" & intProRecID)
            'MsgBox intItmCnt & " item listings for this promotion."
        If intItmCnt = 0 Then
            'Next x, just in case, I thru in a zero
        Else
            Z_Compile = "" 'Start empty
            stInfo = ""
                'MsgBox "Flag 1"
                'MsgBox x & " [RecID] is " & intProRecID
            'Run Macro- Del/Make [zItmTmp].
            'Run [xSSDTmpRecIDAQ]
            'Append [CodeID].[zItmSub] (an autonumber field) to [ISCode].[zItmTmp], and
            'Append [RecID].[zItmSub] to [RecID].[zItmTmp] WHERE [RecID].[zItmSub] = ZRecID()
            'that is, the xth [RecID], which is, the link to the promotion
            'Specifically: Z_RecID = intProRecID = DLookup("[RecID]", "zItemCompile", "[CodeID] =" & x)
            DoCmd.RunMacro "zItmTmpM"        'MsgBox "Copied [zItmTmp]"
                'MsgBox "Flag 2"
                'Z_RecID = DLookup("[RecID]", "zItemSub", "[CodeID] = '" & y & "'")
                'MsgBox intItmCnt & " item listings for this promotion."
            For y = 1 To intItmCnt '1 to the End of Valid Item Codes for this promotion.
                'Start with getting the ID ([CodeID].[zItmSub] = [ISCode].[zItmTmp]) of the field we
                'want to fill with data.
                If Not IsNull(DLookup("[ISCode]", "zItmTmp", "[CodeID]=" & y)) Then
                    'MsgBox "Flag 1.A., Not Null"
                    intISCode = DLookup("[ISCode]", "zItmTmp", "[CodeID]=" & y) 'THIS WAS THE LINE THAT OVERFLOWS
                    'MsgBox "Flag 3, Rec y = " & y & ", [zItmTmp].[CodeID]: " & intISCode
                    
                    If IsNull(DLookup("[HSICode]", "zItemSub", "[CodeID] =" & intISCode)) Then
                        'MsgBox "Flag 2.A., Is Null"
                        'MsgBox "Flag 3A"
                        stInfo = ""  'Next y
                    Else
                        'MsgBox "Flag 2.B., Not Null"
                        'MsgBox "Flag 3B, Rec: " & y & ", HSICode: " & stInfo
                        stInfo = DLookup("[HSICode]", "zItemSub", "[CodeID] =" & intISCode)
                        'MsgBox "3B Item is " & stInfo & " and [zItemSub].[CodeID] = " & intISCode
                    End If
                
                    If ZCompile() = "" And stInfo > "" Then
                        Z_Compile = stInfo
                    ElseIf ZCompile > "" And stInfo > "" Then
                        Z_Compile = ZCompile() & ", " & stInfo
                    Else
                        Z_Compile = ZCompile()
                    End If
                        'Z_Compile = ZCompile() & stInfo
                        'MsgBox "4. Item(s) = " & ZCompile()
                Else
                    'MsgBox "Flag 1.B., It Is Null"
                End If
            Next y
            'Update [zItemCompile] from ZCompile() where [RecID] = RecID()
            If ZCompile() > "" Then
                'MsgBox "5. Item List: " & ZCompile()
                DoCmd.OpenQuery "xSSDTmpUQ"
            End If
        End If
        'For y
        'If x = 3 Then
        '    MsgBox "Hit 3, Closing routine"
        '    Exit Sub
        'End If
    End If
Next x
MsgBox "Update Complete.  Data for Report #2 (includes a comma separated " & _
"string of SSD item codes) has been updated and you may run the report."

Exit Sub
    stDocName = "xSSDProEffQtrYrRw/IC"
    DoCmd.OpenReport stDocName, acPreview

